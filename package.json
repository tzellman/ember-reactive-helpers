{
  "name": "ember-reactive-helpers",
  "version": "1.0.0",
  "description": "Collection of helpers to aid in reactive template programming with Ember.js",
  "keywords": [
    "ember-addon",
    "helpers",
    "r",
    "shhh",
    "transition-to",
    "tap",
    "log"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:EmberSherpa/ember-reactive-helpers.git"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Taras Mankovski",
      "email": "tarasm@gmail.com"
    },
    {
      "name": "Tom Zellman",
      "email": "tzellman@gmail.com"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "ember release"
  },
  "dependencies": {
    "ember-auto-import": "^2.4.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/test-setup": "^0.50.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~5.12.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^3.0.1",
    "ember-math-helpers": "^4.0.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^13.0.2",
    "ember-source": "^5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "husky": "4.3.8",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "qunit": "^2.17.2",
    "qunit-dom": "^3.2.1",
    "webpack": "^5.66.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://EmberSherpa.github.io/ember-reactive-helpers"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --config .prettierrc"
    }
  }
}
